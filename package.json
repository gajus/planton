    "roarr": "^7.0.3",
    "serialize-error": "^8.1.0"
  },
  "description": "Database-agnostic task scheduler.",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^16.4.3",
    "@types/sinon": "^10.0.2",
    "ava": "^3.15.0",
    "coveralls": "^3.1.1",
    "del-cli": "^4.0.1",
    "delay": "^5.0.0",
    "eslint": "^7.31.0",
    "eslint-config-canonical": "^26.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.4",
    "sinon": "^11.1.2",
